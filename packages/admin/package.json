{
  "name": "cloudbase-cms",
  "version": "2.0.0-alpha.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "deploy": "npm run site && npm run gh-pages",
    "dev": "npm run start:dev",
    "postinstall": "umi g tmp",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none UMI_UI=none umi dev",
    "start:pre": "cross-env REACT_APP_ENV=pre umi dev",
    "tsc": "tsc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "@ant-design/pro-card": "^1.0.2",
    "@ant-design/pro-layout": "^6.4.14",
    "@ant-design/pro-list": "^0.0.8",
    "@ant-design/pro-table": "^2.7.4",
    "@umijs/hooks": "^1.9.3",
    "antd": "^4.5.4",
    "braft-editor": "^2.3.9",
    "braft-utils": "^3.0.12",
    "classnames": "^2.2.6",
    "concent": "^2.9.14",
    "css.gg": "^2.0.0",
    "lodash": "^4.17.19",
    "moment": "^2.27.0",
    "omit.js": "^2.0.2",
    "qs": "^6.9.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet-async": "^1.0.6",
    "umi": "^3.2.14",
    "umi-request": "^1.3.5",
    "use-merge-value": "^1.0.2",
    "vditor": "^3.4.6"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.0.2",
    "@types/braft-editor": "^2.2.0",
    "@types/classnames": "^2.2.7",
    "@types/express": "^4.17.0",
    "@types/history": "^4.7.2",
    "@types/lodash": "^4.14.144",
    "@types/qs": "^6.5.3",
    "@types/react": "^16.9.44",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.1.0",
    "@umijs/plugin-blocks": "^2.2.1",
    "@umijs/plugin-esbuild": "^1.0.1",
    "@umijs/preset-ant-design-pro": "^1.2.2",
    "@umijs/preset-react": "^1.5.22",
    "@umijs/preset-ui": "^2.2.6",
    "concent-plugin-redux-devtool": "^2.0.2",
    "cross-env": "^7.0.2",
    "cross-port-killer": "^1.2.1",
    "detect-installer": "^1.0.1",
    "express": "^4.17.1",
    "pro-download": "1.0.1"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
